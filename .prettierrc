{
    "plugins": ["prettier-plugin-blade", "prettier-plugin-tailwindcss"],
    "printWidth": 100,
    "tabWidth": 4,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "htmlWhitespaceSensitivity": "ignore",
    "bladeIndentInnerHtml": true,
    "bladeWrapAttributes": "preserve",
    "bladeSortHtmlAttributes": "none",
    "endOfLine": "lf"
}
